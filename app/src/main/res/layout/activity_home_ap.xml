<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home_AP_Activity"
    android:background="@drawable/signin_up">

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ADMIN PORTAL"
        android:textColor="@color/black"
        android:textSize="40dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.243" />

    <ImageButton
        android:id="@+id/button7"
        android:layout_width="151dp"
        android:layout_height="137dp"
        android:layout_marginTop="180dp"
        android:layout_marginEnd="152dp"
        android:background="@drawable/roundsquare"
        android:src="@drawable/ic_search"
        app:layout_constraintEnd_toEndOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <ImageButton
        android:id="@+id/button8"
        android:layout_width="151dp"
        android:layout_height="137dp"
        android:src="@drawable/ic_alert"
        android:background="@drawable/roundsquare"
        app:layout_constraintBottom_toBottomOf="@+id/button7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.518"
        app:layout_constraintStart_toEndOf="@+id/button7"
        app:layout_constraintTop_toTopOf="@+id/button7"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/button9"
        android:layout_width="151dp"
        android:layout_height="137dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_menu"
        android:background="@drawable/roundsquare"
        app:layout_constraintEnd_toEndOf="@+id/button7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/button7"
        app:layout_constraintTop_toBottomOf="@+id/button7" />

    <ImageButton
        android:id="@+id/button10"
        android:layout_width="151dp"
        android:layout_height="137dp"
        android:background="@drawable/roundsquare"
        android:src="@drawable/ic_complaints"
        app:layout_constraintBottom_toBottomOf="@+id/button9"
        app:layout_constraintEnd_toEndOf="@+id/button8"
        app:layout_constraintTop_toTopOf="@+id/button9" />

    <ImageButton
        android:id="@+id/button11"
        android:layout_width="151dp"
        android:layout_height="137dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/roundsquare"
        android:src="@drawable/ic_list"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintEnd_toEndOf="@+id/button7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/button7" />

    <ImageButton
        android:id="@+id/button12"
        android:layout_width="151dp"
        android:layout_height="137dp"
        android:background="@drawable/roundsquare"
        android:src="@drawable/ic_thumbs_up_down"
        app:layout_constraintBottom_toBottomOf="@+id/button11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button11"
        app:layout_constraintTop_toTopOf="@+id/button11" />

</androidx.constraintlayout.widget.ConstraintLayout>